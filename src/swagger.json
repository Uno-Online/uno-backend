{
    "openapi": "3.0.0",
    "info": {
      "title": "API do UNO",
      "version": "1.0.0",
      "description": "Documentação do jogo online UNO"
    },
    "servers": [
      {
        "url": "http://localhost:3000"
      }
    ],
    "paths": {
      "/authentication/register": {
        "post": {
          "tags": ["authentication"],
          "summary": "Registrar usuário na aplicação",
          "description": "",
          "consumes": ["application/json"],
          "produces": ["application/json"],
          "parameters": [
            {
              "in": "formData",
              "name": "username",
              "required": true,
              "type": "string",
              "example": "johndoe"
            },
            {
              "in": "formData",
              "name": "email",
              "required": true,
              "type": "string",
              "example": "johndoe@example.com"
            },
            {
              "in": "formData",
              "name": "password",
              "type": "string",
              "required": true,
              "format": "password",
              "example": "password"
            }
          ],
          "responses": {
            "201": {
              "description": "Usuário criado com sucesso",
              "schema": {}
            }
          }

        }
      }

    }
  }
  